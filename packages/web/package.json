{
    "name": "@lario/web",
    "version": "1.0.0",
    "private": true,
    "description": "XO Sports Web App written using the GatsbyJS framework.",
    "author": "Lario Borges",
    "keywords": [
        "gatsby"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "scripts": {
        "test": "jest spec.js",
        "develop": "gatsby develop",
        "start": "gatsby develop",
        "build": "gatsby clean && gatsby build",
        "build-staging": "gatsby clean && cross-env STAGING=true gatsby build",
        "build-preprod": "gatsby clean && cross-env PREPROD=true gatsby build",
        "build-prod": "gatsby clean && cross-env PROD=true gatsby build",
        "deploy-staging": "npm run -n \"-r dotenv/config\" && npm run build-staging && cross-env STAGING=true gatsby-plugin-s3 deploy",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "lint": "yarn run lint-js && yarn run lint-styles",
        "lint-styles": "stylelint '{src,plugins}/**/*.scss' --fix",
        "lint-js": "eslint \"{src,apps,libs,test}/**/*.{js,json}\" --fix"
    },
    "dependencies": {
        "@emotion/react": "^11.7.1",
        "@fontsource/poppins": "^4.5.0",
        "@mui/icons-material": "^5.2.5",
        "@mui/material": "^5.2.7",
        "@lario/redux": "workspace:^",
        "gatsby": "^4.2.0",
        "gatsby-plugin-canonical-urls": "^4.4.0",
        "gatsby-plugin-manifest": "^4.3.0",
        "gatsby-plugin-material-ui": "^4.1.0",
        "gatsby-plugin-offline": "^5.3.0",
        "gatsby-plugin-react-helmet": "^5.2.0",
        "gatsby-plugin-robots-txt": "^1.6.14",
        "gatsby-plugin-s3": "^0.3.8",
        "gatsby-plugin-sass": "^5.2.0",
        "gatsby-plugin-sitemap": "^5.2.0",
        "gatsby-theme-material-ui": "^5.1.0",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "react-redux": "^7.2.6",
        "redux-persist": "^6.0.0",
        "reselect": "^4.1.5"
    },
    "devDependencies": {
        "@babel/runtime": "^7.16.5",
        "@jest/globals": "^27.4.4",
        "@lario/eslint-config": "workspace:^",
        "@lario/prettier-config": "workspace:^",
        "axios": "^0.24.0",
        "babel-jest": "^27.4.5",
        "babel-preset-gatsby": "^2.4.0",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "eslint": "^8.5.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-webpack-plugin": "^3.1.1",
        "gatsby-plugin-eslint": "^4.0.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.4.5",
        "jest-config": "^27.4.5",
        "prettier": "^2.5.1",
        "react-test-renderer": "^17.0.2",
        "sass": "^1.43.4",
        "stylelint": "^14.1.0",
        "stylelint-config-standard-scss": "^3.0.0"
    },
    "prettier": "@lario/prettier-config"
}
